{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "#fd9636",
          "foreground": "#fff",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "trailing_diamond": "\ue0b0",
          "template": "{{ .UserName }} ",
          "type": "os"
        },
        {
          "type": "path",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#39aae2",
          "trailing_diamond": "\ue0b0",
          "template": " \ue5ff {{ .Path }} ",
          "properties": {
            "style": "full",
            "prefix": "/",
            "mapped_locations": []
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#193549",
          "background": "#a09de8",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\ue725 "
          }
        },
        {
          "background": "#6864dd",
          "foreground": "#fff",
          "properties": {
            "time_format": "15:04"
          },
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": " \ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#ffff66",
          "template": " \uf0e7 "
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#7adf53",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "trailing_diamond": "\ue0b4",
          "template": " \ue23a{{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ]
}